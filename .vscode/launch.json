{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Project QT6",
			"type": "python",
			"request": "launch",
			"module": "uchoose.uchoose",
			"cwd": "${workspaceFolder}",
			"args": [
				"-u",
				"qt6"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Project QT5",
			"type": "python",
			"request": "launch",
			"module": "uchoose.uchoose",
			"cwd": "${workspaceFolder}",
			"args": [
				"-u",
				"qt5"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Project GTK",
			"type": "python",
			"request": "launch",
			"module": "uchoose.uchoose",
			"cwd": "${workspaceFolder}",
			"args": [
				"-u",
				"gtk"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Project CLI",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/uchoose/uchoose.py",
			"args": [
				"--cli"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Module",
			"type": "python",
			"request": "launch",
			"module": "uchoose.uchoose",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Project (Integrated Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/uchoose/uchoose.py",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Current File (External Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "externalTerminal"
		}
	]
}